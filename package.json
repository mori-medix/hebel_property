{
  "name": "my-package",
  "version": "1.0.0",
  "private": true,
  "description": "this package is used to create a website",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "eslint": "eslint ./src/assets/ts/**/*.ts",
    "eslint:fix": "eslint --fix ./src/assets/ts/**/*.ts",
    "stylelint": "stylelint ./src/**/*.scss",
    "stylelint:fix": "stylelint --fix ./src/**/*.scss",
    "webpack-dev": "webpack -w --config webpack.dev.js",
    "webpack-prod": "webpack --config webpack.prod.js",
    "polyfill": "node polyfill.js",
    "build-html": "ejs-cli --base-dir src/pages/ \"/**/**.ejs\" --out dist/",
    "build-css": "node buildcss.js map:true",
    "build-css:nomap": "node buildcss.js",
    "image-min": "node imagemin.js",
    "webp": "node webp.js",
    "watch-html": "chokidar \"src/**/*.ejs\" -c \"yarn build-html\"",
    "watch-css": "chokidar \"src/**/*.scss\" -c \"yarn build-css\"",
    "clean": "rimraf ./dist/**/css/*.map ./dist/**/js/*.map",
    "format": "js-beautify -f ./dist/**/*.html -r --type[\"html\"]",
    "start": "browser-sync start --server \"./dist/\" --startPath \"./hebel-rooms/search/list_a\" --files \"./dist/**/*.html,./dist/hebel-rooms/common_v2/css/**/*.css,./dist/hebel-rooms/common_v2/js/**/*.js\"",
    "build": "npm-run-all -p build-html build-css:nomap webpack-prod clean",
    "dev": "npm-run-all -p watch-css watch-html webpack-dev start",
    "lint": "npm-run-all -p eslint:fix stylelint:fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.29.3",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "cssnano": "^6.0.1",
    "ejs-cli": "^2.2.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "fs-extra": "^11.1.1",
    "imagemin": "^8.0.1",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-webp": "^8.0.0",
    "js-beautify": "^1.14.8",
    "markuplint": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "postcss-media-minmax": "^5.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.63.4",
    "stylelint": "^15.7.0",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "tailwindcss": "^3.3.2",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webp-converter": "^2.3.3",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11",
    "iOS >= 12",
    "Android >= 4"
  ],
  "dependencies": {
    "intersection-observer": "^0.12.0",
    "micromodal": "^0.4.10",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "eslintIgnore": [
    "src/**/polyfill-library.js"
  ]
}
