      function countdown(tableAccordion){
        if(tableAccordion !== null){
            const text = tableAccordion.querySelector('.property-detail-table-foot_text').textContent;
            const numbers = text.match(/\d+/g);
            tableAccordion.querySelector('.property-detail-table-foot_text').textContent = text.replace(/\d+/g, numbers - 1);
          }
      };
      function propertyDetailRemove(event){
        event.preventDefault;
        const target = event.currentTarget;
        const content = target.closest('.property-detail');
        const tableAccordion = content.querySelector('.js-table-accordion');
        if( target.closest('.property-detail_table-body').childElementCount === 1){
            const table = target.closest('.js-table');
            target.closest('.property-detail_table-body').remove();
            tableAccordion.style.display = 'none';
            if(table.querySelectorAll('.property-detail_table-body').length === 0){
            content.remove();
            }
        } else {
          target.closest('.property-detail_table-body-inner').remove();
          countdown(tableAccordion);
        }
      }
      function contentsRemove(event){
        event.preventDefault;
        event.currentTarget.closest('._searchbox_history_list-property-item').remove();
      }
